{
  "name": "vue-class-store",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A class based Vue 3 State/Store Manager",
  "author": {
    "name": "Joao Sumbo",
    "email": "joaosumbo34@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "Rollup:Build": "rimraf dist && rollup -c rollup.config.js "
  },
  "main": "dist/vue-class-store-plugin.cjs.js",
  "module": "dist/vue-class-store-plugin.esm-bundler.js",
  "unpkg": "dist/vue-class-store-plugin.global.js",
  "jsdelivr": "dist/vue-class-store-plugin.global.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": ".dist/vue-class-store-plugin.cjs.js",
      "browser": ".dist/vue-class-store-plugin.esm-browser.js",
      "import": ".dist/vue-class-store-plugin.esm-bundler.js",
      "module": ".dist/vue-class-store-plugin.esm-bundler.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "dependencies": {
    "@vue/devtools-api": "^6.6.1",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "pascalcase": "^2.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.4.15"
  }
}
